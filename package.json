{
  "name": "@bakesaled/moose-watch",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch",
    "test:unit:ci": "ng test --code-coverage",
    "test:dev": "ng test --source-map=false",
    "test:ci": "npm run lint && npm run test:unit:ci && npm run e2e",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ngh",
    "_prettier": "prettier --print-width 80 --tab-width 2 --single-quote --trailing-comma none",
    "prettier": "npm run _prettier -- --write  \"src/**/*.ts\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "clean:lib": "del dist-lib",
    "clean:static:lib": "del 'dist-lib/**/*.html' 'dist-lib/**/*.scss' 'dist-lib/**/*.js' 'dist-lib/**/*.js.map' '!dist-lib/bundles/*.js' '!dist-lib/bundles/*.js.map'",
    "transpile:lib": "ngc -p ./tsconfig.aot.json",
    "build:lib": "npm run clean:lib && npm run transpile:lib && npm run copy:static:lib && npm run package:lib:packagr",
    "build:prod": "ng build --prod --vendor-chunk=true --output-path dist",
    "build:prod:analyze": "npm run build:prod -- --stats-json",
    "build:ci": "npm run build:prod -- --base-href \"https://bakesaled.github.io/$NAME/\"",
    "copy:static:lib": "cd src/lib && copyfiles \"./**/*.html\" \"./**/*.scss\" \"package.json\" ../../dist-lib/ && cd /",
    "package:lib:packagr": "cd src/lib && ng-packagr -p ng-package.json && cd /",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "core-js": "^2.5.7",
    "ng2-dnd": "^5.0.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "^6.3.3",
    "tsickle": "^0.27.5",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "^6.2.4",
    "@angular/compiler": "^6.1.9",
    "@angular/compiler-cli": "^6.1.9",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "^2.0.5",
    "codelyzer": "^4.5.0",
    "copyfiles": "^1.2.0",
    "coveralls": "^3.0.2",
    "del-cli": "^1.1.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.3.0",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "typescript": "2.9.2",
    "webpack-bundle-analyzer": "^3.0.0"
  }
}
